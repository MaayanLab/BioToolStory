{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/entity.json"
  },
  "properties": {
    "Name": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Name']}"
    },
    "Gene ID": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Gene_ID']}"
    },
    "Taxon ID": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Taxon_ID']}"
    }
  }
}