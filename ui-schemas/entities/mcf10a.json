{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/entity.json"
  },
  "properties": {
    "Ensembl Gene ID": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['ensembl_gene_id']}"
    },
    "HGNC symbol": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['hgnc_symbol']}"
    },
    "MDD": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['MDD']}"
    },
    "Pathway": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Pathway']}"
    },
    "probeset": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['probeset']}"
    },
    "Gene Title": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['pr_gene_title']}"
    },
    "histone": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['histone']}"
    },
    "idx": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['idx']}"
    }
  }
}