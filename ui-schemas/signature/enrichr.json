{
  "match": {
    "${$validator}": "/@dcic/signature-commons-schema/core/signature.json",
    "${library.dataset}": "enrichr"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${library.meta['Icon']}",
      "alt": "${library.meta['Library_name']}"
    },
    "Description": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Original_String']}"
    },
    "Assay": {
      "visibility": 1,
      "condition": {
        "${meta.Assay}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.Assay}"
    },
    "Organism": {
      "visibility": 1,
      "condition": {
        "${meta.Organism}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.Organism}"
    },
    "P-Value": {
      "visibility": 1,
      "condition": {
        "${meta['p-value']}": {
          "ne": null
        }
      },
      "columnType": "number",
      "columnOptions": {
        "sort": true,
        "sortDirection": "asc"
      },
      "type": "text",
      "text": "${meta['p-value']}"
    },
    "Odds Ratio": {
      "visibility": 1,
      "condition": {
        "${meta.oddsratio}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.oddsratio}"
    },
    "Set Size": {
      "visibility": 1,
      "condition": {
        "${meta.setsize}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.setsize}"
    },
    "Cell-line": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Cell_Line}",
      "subfield": "${meta.Cell_Line[0].Name}"
    },
    "Tissue": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Tissue}",
      "subfield": "${meta.Tissue[0].Name}"
    },
    "Antibody_Identifier": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Antibody_Identifier}"
    },
    "Disease": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Disease}",
      "subfield": "${meta.Disease[0].Name}"
    },
    "Gene": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Gene}",
      "subfield": "${meta.Gene[0].Name}"
    },
    "Metabolite": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Metabolite}",
      "subfield": "${meta.Metabolite[0].Name}"
    },
    "MicroRNA": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.MicroRNA}"
    },
    "Phenotype": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Phenotype}",
      "subfield": "${meta.Phenotype[0].Name}"
    },
    "PTM": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.PTM}"
    },
    "Small Molecule": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Small_Molecule}",
      "subfield": "${meta.Small_Molecule[0].Name}"
    },
    "Virus": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Virus}",
      "subfield": "${meta.Virus[0].Name}"
    }
  }
}