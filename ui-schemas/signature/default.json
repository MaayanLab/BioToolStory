{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json",
    "dataset": "default"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "alt": "${library.meta['Library_name']}",
      "src": "${PREFIX}/static/images/default-black.png"
    },
    "Info": {
      "visibility": 1,
      "type": "text-default",
      "text": "${Object.keys(meta)[0]}: ${meta[Object.keys(meta)[0]]}"
    },
    "Info 2": {
      "visibility": 1,
      "type": "text-default",
      "text": "${Object.keys(meta)[1]}: ${meta[Object.keys(meta)[1]]}"
    },
    "P-Value": {
      "visibility": 1,
      "condition": {
        "${meta['p-value']}": {
          "ne": null
        }
      },
      "columnType": "number",
      "columnOptions": {
        "sort": true,
        "sortDirection": "asc"
      },
      "type": "text",
      "text": "${meta['p-value']}"
    },
    "Odds Ratio": {
      "visibility": 1,
      "condition": {
        "${meta.oddsratio}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.oddsratio}"
    },
    "Set Size": {
      "visibility": 1,
      "condition": {
        "${meta.setsize}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.setsize}"
    }
  }
}