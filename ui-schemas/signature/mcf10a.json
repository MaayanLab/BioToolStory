{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json",
    "${library.dataset}": "MCF10A*"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${library.meta['Icon']}",
      "alt": "${library.meta['assay']}"
    },
    "Specimen Name": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['specimenName']}"
    },
    "Cell Type": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['cellType']}"
    },
    "Ligand": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['ligand']}"
    },
    "P-Value": {
      "visibility": 1,
      "condition": {
        "${meta['p-value']}": {
          "ne": null
        }
      },
      "columnType": "number",
      "columnOptions": {
        "sort": true,
        "sortDirection": "asc"
      },
      "type": "text",
      "text": "${meta['p-value']}"
    },
    "Odds Ratio": {
      "visibility": 1,
      "condition": {
        "${meta.oddsratio}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.oddsratio}"
    },
    "Set Size": {
      "visibility": 1,
      "condition": {
        "${meta.setsize}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.setsize}"
    }
  }
}