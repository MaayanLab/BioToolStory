{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json",
    "${dataset}": "MCF10A*"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${meta['Icon']}",
      "alt": "${meta['assay']}",
      "priority": 1
    },
    "Assay": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['assay']}",
      "priority": 2
    },
    "Institution": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['institution']}",
      "priority": 2
    },
    "LINCS center": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['LINCS_center']}",
      "priority": 2
    }
  }
}