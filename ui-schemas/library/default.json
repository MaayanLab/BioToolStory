{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "alt": "${meta['Library_name']}",
      "src": "${PREFIX}/static/images/default-black.png"
    },
    "Library": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Library_name']}"
    }
  }
}