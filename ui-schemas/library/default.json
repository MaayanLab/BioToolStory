{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "alt": "${meta['Description'] || meta['description'] || meta[Object.keys(meta)[0]]}",
      "src": "${PREFIX}/static/images/default-black.png",
      "priority": 1
    }
  }
}