{
  "match": {
    "${$validator}": "/@dcic/signature-commons-schema/core/library.json",
    "${dataset}": "creeds"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${meta['Icon']}",
      "alt": "${meta['Primary_Resource']}"
    },
    "Library": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Library_name']}"
    }
  }
}