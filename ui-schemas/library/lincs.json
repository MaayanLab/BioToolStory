{
  "match": {
    "${$validator}": "/dcic/signature-commons-schema/(v[0-9]+/)?core/library.json",
    "${dataset}": "lincs_clue"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${meta['Icon']}",
      "alt": "${meta['Primary_Resource']}"
    },
    "Library": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Library_name']}"
    }
  }
}