{
  "$id": "/dcic/signature-commons-schema/v5/meta/library/sigcom.json",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "allOf": [
    {
      "$ref": "https://raw.githubusercontent.com/MaayanLab/btools-ui/master/btools_journal.json"
    },
    {
      "properties": {
            "Journal_Title": {
                        "description": "journal name",
                        "type": "string"
                    },
            "ISSN": {
                        "description": "electronic ISSN",
                        "type": "string"
                    },
            "publisher": {
                        "description": "journal publisher",
                        "type": "string"
                    },
            "icon": {
                        "description": "path to journal symbol",
                        "type": "string",
                        "format":"uri"
                    }
        },
      "required": [
            "$validator",
            "Journal_Title",
            "ISSN"
        ],
      "type": "object"
    }
  ]
}
