  "scripts": {
    "dev": "cross-env PREFIX=${PREFIX} next",
    "build": "cross-env PREFIX=${PREFIX} next build",
    "start": "cross-env PREFIX=${PREFIX} next start",
    "export": "cross-env PREFIX=${PREFIX} next build && cross-env PREFIX=${PREFIX} next export",
    "export:production": "cross-env PREFIX=/sigcom npm run export",
    "export:dev": "cross-env PREFIX=/sigcom-dev npm run export",
    "deploy": "cross-env REACT_APP_METADATA_API= REACT_APP_DATA_API= PREFIX=${PREFIX} npm run export && docker build --build-arg PREFIX=${PREFIX} -t ${TAG}:${VERSION} . && docker push ${TAG}:${VERSION} && docker tag ${TAG}:${VERSION} ${TAG}:latest && docker push ${TAG}:latest",
    "deploy:production": "cross-env PREFIX=/sigcom TAG=maayanlab/sigcom VERSION=${npm_package_version} npm run deploy",
    "deploy:dev": "cross-env PREFIX=/sigcom-dev TAG=maayanlab/sigcom-dev VERSION=${npm_package_version} npm run deploy"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "next-build-id": "^2.0.1",
    "node-sass": "^4.11.0"
  }
}
