{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/entity.json"
  },
  "properties": {
    "Name": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.symbol}",
      "priority": 1
    },
    "Gene ID": {
      "visibility": 2,
      "type": "text",
      "text": "${meta.geneid}",
      "priority": 2
    },
    "Taxon ID": {
      "visibility": 2,
      "type": "text",
      "text": "${meta.taxid}",
      "priority": 3
    }
  }
}