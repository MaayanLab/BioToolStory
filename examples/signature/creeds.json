{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json",
    "${library.dataset}": "lincs_fwd"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/static/images/creeds.png",
      "alt": "CREEDS",
      "priority": 1,
      "icon": true
    },
    "Library": {
      "visibility": 0,
      "type": "text",
      "text": "CREEDS",
      "priority": 1,
      "subtitle": true
    },
    "ID": {
      "visibility": 0,
      "type": "text",
      "text": "${meta.id}",
      "priority": 1,
      "name": true
    },
    "Assay": {
      "visibility": 1,
      "type": "text",
      "text": "Microarray",
      "priority": 1,
      "MDI_Icon": "mdi-flask-outline"
    },
    "Disease": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Disease[0].Name}",
      "priority": 2,
      "MDI_Icon": "mdi-emoticon-cry-outline"
    },
    "Gene": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Gene[0].Name}",
      "priority": 2,
      "MDI_Icon": "mdi-dna"
    },
    "Drug": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Small_Molecule[0].Name}",
      "priority": 2,
      "MDI_Icon": "mdi-hexagon-multiple"
    },
    "DrugBank": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Small_Molecule[0].DrugBankID}",
      "priority": 3,
      "MDI_Icon": "mdi-pill"
    },
    "Organism": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Organism}",
      "priority": 3,
      "MDI_Icon": "mdi-human-child"
    },
    "GEO Accession": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.geo_id}",
      "priority": 3,
      "MDI_Icon": "mdi-identifier"
    },
    "P-Value": {
      "visibility": 1,
      "condition": {
        "${meta['p-value']}": {
          "ne": null
        }
      },
      "columnType": "number",
      "columnOptions": {
        "sort": true,
        "sortDirection": "asc"
      },
      "type": "text",
      "text": "${meta['p-value']}",
      "priority": 5
    },
    "Odds Ratio": {
      "visibility": 1,
      "condition": {
        "${meta.oddsratio}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.oddsratio}",
      "priority": 6
    },
    "Gene Set Size": {
      "visibility": 1,
      "condition": {
        "${meta.setsize}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.setsize}",
      "priority": 4
    }
  }
}