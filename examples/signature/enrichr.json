{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/signature.json",
    "${library.dataset}": "enrichr_geneset",
    "${library.meta.Library_name}": "^((?!LINCS).)*$"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${library.meta['Icon']}",
      "alt": "${library.meta['Library_name']}",
      "priority": 1,
      "icon": true
    },
    "Library": {
      "visibility": 1,
      "type": "text",
      "text": "${library.meta['Library_name']}",
      "priority": 1,
      "parent": true
    },
    "Description": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Original_String']}",
      "subfield": "${meta.Pathway.Name}",
      "title": true,
      "priority": 2
    },
    "Gene": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Gene}",
      "subfield": "${meta.Gene[0].Name}",
      "priority": 3
    },
    "Disease": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Disease}",
      "subfield": "${meta.Disease[0].Name}",
      "priority": 3
    },
    "Pathway": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Pathway}",
      "subfield": "${meta.Pathway.Name}",
      "priority": 3
    },
    "Metabolite": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Metabolite}",
      "subfield": "${meta.Metabolite.Name}",
      "priority": 3
    },
    "MicroRNA": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.MicroRNA}",
      "priority": 3
    },
    "Antibody_Identifier": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Antibody_Identifier}",
      "priority": 3
    },
    "Phenotype": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Phenotype}",
      "subfield": "${meta.Phenotype[0].Name}",
      "priority": 3
    },
    "PTM": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.PTM}",
      "priority": 3
    },
    "Small Molecule": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Small_Molecule}",
      "subfield": "${meta.Small_Molecule[0].Name}",
      "priority": 3
    },
    "Virus": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Virus}",
      "subfield": "${meta.Virus[0].Name}",
      "priority": 3
    },
    "Cell-line": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Cell_Line}",
      "subfield": "${meta.Cell_Line[0].Name}",
      "priority": 4
    },
    "Tissue": {
      "visibility": 1,
      "type": "object",
      "text": "${meta.Tissue}",
      "subfield": "${meta.Tissue[0].Name}",
      "priority": 4
    },
    "Assay": {
      "visibility": 1,
      "condition": {
        "${meta.Assay}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.Assay}",
      "priority": 5
    },
    "Organism": {
      "visibility": 1,
      "condition": {
        "${meta.Organism}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.Organism}",
      "priority": 5
    },
    "P-Value": {
      "visibility": 1,
      "condition": {
        "${meta['p-value']}": {
          "ne": null
        }
      },
      "columnType": "number",
      "columnOptions": {
        "sort": true,
        "sortDirection": "asc"
      },
      "type": "text",
      "text": "${meta['p-value']}",
      "priority": 6
    },
    "Odds Ratio": {
      "visibility": 1,
      "condition": {
        "${meta.oddsratio}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.oddsratio}",
      "priority": 6
    },
    "Set Size": {
      "visibility": 1,
      "condition": {
        "${meta.setsize}": {
          "ne": null
        }
      },
      "columnType": "number",
      "type": "text",
      "text": "${meta.setsize}",
      "priority": 6
    }
  }
}