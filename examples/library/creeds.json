{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json",
    "${dataset}": "lincs_fwd"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${meta['Icon']}",
      "alt": "${meta['Primary_Resource']}",
      "priority": 1,
      "icon": true
    },
    "Library": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Library_name']}",
      "priority": 2,
      "name": true
    },
    "Description": {
      "visibility": 0,
      "type": "text",
      "text": "${meta['Description']}",
      "priority": 2,
      "description": true
    }
  }
}