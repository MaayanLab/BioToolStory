{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json",
    "${dataset_type}": "geneset_library"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${meta.creators[0].affiliations[0].extraProperties[0].values[0]}",
      "alt": "${meta.title || meta.libraryName}",
      "priority": 1,
      "icon": true,
      "name": true
    },
    "Assay": {
      "visibility": 1,
      "type": "text",
      "alt": "${meta.types.method.value}",
      "priority": 2
    },
    "Institute": {
      "visibility": 1,
      "type": "text",
      "alt": "${meta.creators.affiliations.name}",
      "priority": 4
    },
    "Library ID": {
      "visibility": 1,
      "type": "text",
      "alt": "${meta.libraryID}",
      "priority": 3
    },
    "Signature Count": {
      "visibility": 1,
      "type": "text",
      "alt": "${meta.signatureCount}",
      "priority": 5
    }
  }
}