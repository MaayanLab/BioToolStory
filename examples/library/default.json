{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/library.json"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "alt": "${id}",
      "src": "${PREFIX}/static/images/default-black.png",
      "priority": 1,
      "icon": true,
      "name": true
    },
    "Description": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Description || meta.description}",
      "priority": 2
    }
  }
}