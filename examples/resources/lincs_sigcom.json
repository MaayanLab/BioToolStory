{
  "match": {
    "${$validator}": "/@?dcic/signature-commons-schema/(v[0-9]+/)?core/resource.json"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "${PREFIX}/${meta.extraProperties[0].values[0]}",
      "alt": "${meta.name}",
      "text": "${meta.name}",
      "priority": 1,
      "icon": true,
      "name": true
    },
    "Description": {
      "visibility": 1,
      "type": "text",
      "field": "Description",
      "text": "${meta.description}",
      "priority": 2
    },
    "URL": {
      "visibility": 1,
      "type": "text",
      "field": "URL",
      "text": "${meta.access[0].landingPage}",
      "priority": 3
    },
    "Signature Count": {
      "visibility": 1,
      "type": "text",
      "field": "Signature_Count",
      "text": "${meta['Signature_Count']}",
      "priority": 3
    }
  }
}