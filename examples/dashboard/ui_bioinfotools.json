[
  {
    "landing": true,
    "content": {
      "favicon": {
        "src": "/static/btools.png",
        "alt": "BTools",
        "title": "BTools: Bioinformatics Tool Search"
      },
      "header_info": {
          "icon": {
              "src": "/static/btools.png",
              "alt": "BTools",
              "style": {
                "width": 100
              }
          },
          "header_left": "B",
          "header_right": "ls"
      },
      "metadata_placeholder": "Search over thousands of tools",
      "text_1": "Explore an extensive collection of bioinformatics tools",
      "text_4": " ",
      "preferred_name_singular": {
        "signatures": "Tool",
        "libraries": "Journal"
      },
      "preferred_name": {
        "signatures": "Tools",
        "libraries": "Journals"
      },
      "nav": {
        "MetadataSearch": {
          "active": true,
          "endpoint": "/ToolSearch"
        },
        "SignatureSearch": {
          "active": false
        },
        "Resources": {
          "active": true,
          "endpoint": "/Journals"
        },
        "API": {
          "active": true
        }
      },
      "order_default": {
        "signatures": "meta.total_pubmed_citations"
      },
      "footer_links": [
        {
          "src": "/static/maayanlab.png",
          "alt": "maayan",
          "href": "http://labs.icahn.mssm.edu/maayanlab/",
          "containerProps": {
            "style": {
              "width": 100
            }
          },
          "imageProps": {
            "style": {
              "width": 100
            }
          }
        },
        {
          "src": "/static/centerbioinfo_light.png",
          "alt": "mssm",
          "href": "https://icahn.mssm.edu/research/bioinformatics"
        }
      ],
      "font_families": [
        "https://fonts.googleapis.com/css?family=Ubuntu&display=swap"
      ],
      "theme_mod": {
        "palette": {
          "primary": {
            "main": "#3d550c"
          },
          "secondary": {
            "main": "#ecf87f"
          },
          "default": {
            "main": "#3d550c",
            "contrastText": "#FFF"
          }
        },
        "chipColors": {
          "default": { "backgroundColor": "#ecf87f", "color": "#000" }
        },
        "typography": {
          "fontFamily": "Ubuntu, sans-serif"
        }
      },
      "github": "https://github.com/MaayanLab/btools-ui",
      "github_issues": "https://github.com/MaayanLab/btools-ui/issues",
      "deactivate_download": true,
      "counting_validator": "/dcic/signature-commons-schema/v5/meta/schema/counting.json",
      "bar_chart_style": {
        "ResponsiveContainer": {
          "width": "100%",
          "height": 300
        },
        "Chart": {
          "margin": {
            "right": 50,
            "left": 50
          }
        },
        "XAxis": {
          "axisLine": false,
          "angle": 45,
          "height": 50,
          "textAnchor": "start",
          "interval": 0,
          "tick": {
            "fontSize": 9
          }
        },
        "Bar": {
          "fill": "#3d550c"
        }
      },
      "pie_chart_style": {
        "Pie": {
          "fill":  "#3d550c"
        },
        "Text_Label": {
          "fontSize": 14
        }
      },
      "text_2": "Search across various fields",
      "text_3": "By",
      "ui_schema": "/dcic/signature-commons-schema/v5/meta/schema/ui-schema.json",
      "search_terms": [
        "visualization",
        "alignment",
        "differential gene expression",
        "docking"
      ]
    }
  },
  {
    "admin": true,
    "content": {
      "preferred_name_singular": {
        "libraries": "Tool",
        "resources": "Journal"
      },
      "bar_chart": {
        "Field_Name": "pub_year",
        "Caption": "Publication Year"
      },
      "bar_chart_style": {
        "ResponsiveContainer": {
          "width": "100%",
          "height": 500
        },
        "Chart": {
          "margin": {
            "right": 50,
            "left": 50
          }
        },
        "XAxis": {
          "axisLine": false,
          "angle": 45,
          "height": 180,
          "textAnchor": "start",
          "interval": 0,
          "tick": {
            "fontSize": 9
          }
        },
        "Bar": {
          "fill": "#75bef5"
        }
      },
      "resource_name_from_library": "journal_name",
      "resource_name": "${meta.name}",
      "resource_icon": "${meta.icon}",
      "library_name": "tool_name",
      "counting_validator": "/dcic/signature-commons-schema/v5/meta/schema/counting.json"
    }
  }
]