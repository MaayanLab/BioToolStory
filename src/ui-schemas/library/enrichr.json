{
  "match": {
    "${$validator}": "/@dcic/signature-commons-schema/core/library.json",
    "${dataset}": "enrichr"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "http://amp.pharm.mssm.edu/Enrichr/images/enrichr-icon.png",
      "alt": "Enrichr"
    },
    "Library Name": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Library name']}"
    }
  }
}