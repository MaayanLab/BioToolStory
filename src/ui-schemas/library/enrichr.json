{
  "match": {
    "${$validator}": "/@dcic/signature-commons-schema/core/library.json",
    "${dataset}": "enrichr"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "http://amp.pharm.mssm.edu/Enrichr/images/enrichr-icon.png",
      "alt": "Enrichr"
    },
    "Assay": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Assay}"
    },
    "Database": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Database}"
    },
    "Organism": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Organism}"
    },
    "Measurement": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.Measurement}"
    },
    "Primary Resource": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Primary Resource']}"
    }
  }
}