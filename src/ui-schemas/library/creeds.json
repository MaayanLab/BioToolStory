{
  "match": {
    "${$validator}": "/@dcic/signature-commons-schema/core/library.json",
    "${dataset}": "creeds"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "http://amp.pharm.mssm.edu/enrichmentapi/images/creeds.png",
      "alt": "CREEDS"
    },
    "Name": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.name}"
    },
    "Assay": {
      "visibility": 1,
      "type": "text",
      "text": "Microarray"
    }
  }
}