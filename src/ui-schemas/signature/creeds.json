{
  "match": {
    "$validator": "/@dcic/signature-commons-schema/core/signature.json",
    "dataset": "CREEDS"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "http://amp.pharm.mssm.edu/CREEDS/img/creeds.png",
      "alt": "CREEDS"
    },
    "P-Value": {
      "visibility": 1,
      "condition": {
        "${meta['p-value']}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta['p-value']}"
    },
    "Odds Ratio": {
      "visibility": 1,
      "condition": {
        "${meta.oddsratio}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.oddsratio}"
    },
    "Set Size": {
      "visibility": 1,
      "condition": {
        "${meta.setsize}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.setsize}"
    },
    "Assay": {
      "visibility": 1,
      "type": "text",
      "text": "Microarray"
    },
    "Drug": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.drug_name}"
    },
    "DrugBank": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.drugbank_id}"
    },
    "Organism": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.organism}"
    },
    "GEO Accession": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.geo_id}"
    },
    "CREEDS ID": {
      "visibility": 1,
      "type": "text",
      "text": "${meta.id}"
    }
  }
}