{
  "match": {
    "${$validator}": "/@dcic/signature-commons-schema/core/signature.json",
    "${library.dataset}": "enrichr"
  },
  "properties": {
    "Image": {
      "visibility": 1,
      "type": "img",
      "src": "http://amp.pharm.mssm.edu/Enrichr/images/enrichr-icon.png",
      "alt": "Enrichr"
    },
    "P-Value": {
      "visibility": 1,
      "condition": {
        "${meta['p-value']}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta['p-value']}"
    },
    "Odds Ratio": {
      "visibility": 1,
      "condition": {
        "${meta.oddsratio}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.oddsratio}"
    },
    "Set Size": {
      "visibility": 1,
      "condition": {
        "${meta.setsize}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.setsize}"
    },
    "Resource": {
      "visibility": 1,
      "condition": {
        "${meta.Resource}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.Resource}"
    },
    "Assay": {
      "visibility": 1,
      "condition": {
        "${meta.Assay}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.Assay}"
    },
    "Organism": {
      "visibility": 1,
      "condition": {
        "${meta.Organism}": {
          "ne": null
        }
      },
      "type": "text",
      "text": "${meta.Organism}"
    },
    "Description": {
      "visibility": 1,
      "type": "text",
      "text": "${meta['Original String']}"
    }
  }
}